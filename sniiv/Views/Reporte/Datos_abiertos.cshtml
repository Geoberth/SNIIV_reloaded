            });

            $('#tab-active').addClass('active');
            $(".lst").click(function () {
                $("#lbl_subtitulo").text($(this).first().text());
            });
        });

        function descargar_dato(tipo, formato) {
            switch (tipo) {
                case _conavi:
                    anio = $('#ddl_anio_s').val();
                    break;
                case _financiamiento:
                    anio = $('#ddl_anio_f').val();
                    break;
                case _ofertaVivienda:
                    anio = $('#ddl_anio_ov').val();
                    break;
                case _registroVivienda:
                    anio = $('#ddl_anio_rv').val();
                    break;
                case _diasInventario:
                    anio = $('#ddl_anio_di').val();
                    break;
                default:
                    anio = $('#ddl_anio_cnbv').val();
                    break;
            }
            $.getJSON(path + 'ReporteAPI/GetDocumentoAnio/' + tipo + '/' + anio + '/' + formato, function (json) {
                window.open(json.documento);
            });
        }

        function mostrar_dato(tipo, formato) {
            var dv, tbl;
            switch (tipo) {
                case _conavi:
                    anio = $('#ddl_anio_s').val();
                    dv = $('#dv_s');
                    tbl = $('#tbl_s');
                    break;
                case _financiamiento:
                    anio = $('#ddl_anio_f').val();
                    dv = $('#dv_f');
                    tbl = $('#tbl_f');
                    break;
                case _ofertaVivienda:
                    anio = $('#ddl_anio_ov').val();
                    dv = $('#dv_ov');
                    tbl = $('#tbl_ov');
                    break;
                case _registroVivienda:
                    anio = $('#ddl_anio_rv').val();
                    dv = $('#dv_rv');
                    tbl = $('#tbl_rv');
                    break;
                case _diasInventario:
                    anio = $('#ddl_anio_di').val();
                    dv = $('#dv_di');
                    tbl = $('#tbl_di');
                    break;
                default:
                    anio = $('#ddl_anio_cnbv').val();
                    dv = $('#dv_cnbv');
                    tbl = $('#tbl_cnbv');
                    break;
            }
            dv.hide();
            $(".loader").show();
            $.getJSON(path + 'ReporteAPI/GetDocumentoAnio/' + tipo + '/' + anio + '/' + formato, function (json) {
                var archivo = 'https://sniiv-cors.herokuapp.com/' + json.documento;
                $.ajax({
                    url: archivo,
                    dataType: 'text',
                }).done(function (data, textStatus, jqXHR) {
                    var allRows = data.split(/\r?\n|\r/);
                    var table = '<table class="table table-striped table-condensed">';
                    for (var singleRow = 0; singleRow < allRows.length; singleRow++) {
                        if (singleRow === 0) {
                            table += '<thead>';
                            table += '<tr>';
                        } else
                            table += '<tr>';
                        var rowCells = allRows[singleRow].split(',');
                        for (var rowCell = 0; rowCell < rowCells.length; rowCell++) {
                            if (singleRow === 0) {
                                table += '<th>';
                                table += rowCells[rowCell];
                                table += '</th>';
                            } else {
                                table += '<td>';
                                table += rowCells[rowCell];
                                table += '</td>';
                            }
                        }
                        if (singleRow === 0) {
                            table += '</tr>';
                            table += '</thead>';
                            table += '<tbody>';
                        } else
                            table += '</tr>';
                        if (singleRow === lim_rows)
                            break;
                    }
                    table += '</tbody>';
                    table += '</table>';

                    dv.show();
                    tbl.empty();
                    tbl.append(table);

                    $('html, body').animate({
                        scrollTop: dv.offset().top - 150
                    }, 500);
                })
                    .fail(function (jqXHR, textStatus, errorThrown) { console.log(errorThrown); })
                    .always(function () {
                        $(".loader").hide();
                    });
            });
        }

        function copiar_dato(tipo, formato) {
            switch (tipo) {
                case _conavi:
                    anio = $('#ddl_anio_s').val();
                    break;
                case _financiamiento:
                    anio = $('#ddl_anio_f').val();
                    break;
                case _ofertaVivienda:
                    anio = $('#ddl_anio_ov').val();
                    break;
                case _registroVivienda:
                    anio = $('#ddl_anio_rv').val();
                    break;
                case _diasInventario:
                    anio = $('#ddl_anio_di').val();
                    break;
                default:
                    anio = $('#ddl_anio_cnbv').val();
                    break;
            }
            $.getJSON(path + 'ReporteAPI/GetDocumentoAnio/' + tipo + '/' + anio + '/' + formato, function (json) {
                var dummy = document.createElement('textarea');
                document.body.appendChild(dummy);
                dummy.value = json.documento;
                dummy.select();
                document.execCommand('copy');
                document.body.removeChild(dummy);

                $('.lbl_copy').fadeIn(900);
                $('.lbl_copy').fadeOut(900);
            });
        }

        function mostrarSubcategoria(tipo) {
            $('.dv_contenido').hide();
            switch (tipo) {
                case _conavi:
                    $('#dv_conavi').show();
                    break;
                case _financiamiento:
                    $('#dv_financiamiento').show();
                    break;
                case _ofertaVivienda:
                    $('#dv_oferta_vivienda').show();
                    break;
                case _registroVivienda:
                    $('#dv_registro_vivienda').show();
                    break;
                case _diasInventario:
                    $('#dv_dias_inventario').show();
                    break;
                default:
                    $('#dv_banca').show();
                    break;
            }
        }

    </script>

