        var anio;
        var _conavi = 1, _financiamiento = 2, _ofertaVivienda = 3, _registroVivienda = 4, _diasInventario = 5, _cnbv = 7;
        var _csv = 1, _xlsx = 2;
        var lim_rows = 100;
        $(function () {
            $(".loader").hide();

            $('#dv_devep').hide();
            $('#img_dev').hide();

            $('#img_conavi').click(function () { mostrarSubcategoria(_conavi); });
            $('#img_financiamiento').click(function () { mostrarSubcategoria(_financiamiento); });
            $('#img_oferta_vivienda').click(function () { mostrarSubcategoria(_ofertaVivienda); });
            $('#img_registro_vivienda').click(function () { mostrarSubcategoria(_registroVivienda); });
            $('#img_dias_inventario').click(function () { mostrarSubcategoria(_diasInventario); });
            $('#img_cnbv').click(function () { mostrarSubcategoria(_cnbv); });

            $('#lnk_f').click(function (e) {
